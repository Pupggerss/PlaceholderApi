{
  "name": "pup/placeholderapi",
  "version": "2.0.0",
  "description": "A simple and efficient placeholder system for PocketMine-MP. Originally created for the Kyro core.",
  "type": "library",
  "keywords": ["pocketmine", "placeholder", "api", "virion"],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Pupggers"
    }
  ],
  "require": {
    "php": ">=8.1",
    "pocketmine/pocketmine-mp": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "pup\\placeholderapi\\": "src/"
    }
  },
  "extra": {
    "virion": {
      "spec": "3.1",
      "namespace-root": "pup\\placeholderapi"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}